         <project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
            <modelVersion>4.0.0</modelVersion>
            <parent>
               <groupId>fr.liglab.adele.common</groupId>
               <artifactId>base</artifactId>
               <version>1.0.3</version>
               <relativePath />
            </parent>
            <artifactId>tools-parent</artifactId>
            <name>${productName} :: Parent</name>
            <version>1.0.0-SNAPSHOT</version>
            <packaging>pom</packaging>
            <url>http://wikiadele.imag.fr/index.php/Cilia</url>
            
            <licenses>
               <license>
                  <name>Apache License, Version 2.0</name>
                  <url>http://www.apache.org/licenses/LICENSE-2.0.html</url>
                  <distribution>manual</distribution>
               </license>
            </licenses>
            
            <organization>
               <name>Adele Team | LIG</name>
               <url>http://adele.imag.fr</url>
            </organization>
            
            <!--Project Repository -->
            <repositories>
               <repository>
                  <id>iCASA.snapshot</id>
                  <name>iCASA Snapshot Repository</name>
                  <url>http://repository-icasa.forge.cloudbees.com/snapshot/</url>
               </repository>
               <repository>
                  <id>iCASA.release</id>
                  <name>iCASA Release Repository</name>
                  <url>http://repository-icasa.forge.cloudbees.com/release/</url>
               </repository>
               
            </repositories>
            
            <pluginRepositories>
               <pluginRepository>
                  <id>icasa-snapshot-repository</id>
                  <name>iCASA Snapshot repository</name>
                  <url>http://repository-icasa.forge.cloudbees.com/snapshot/</url>
               </pluginRepository>
            </pluginRepositories>
            
            <build>
               <pluginManagement>
                  <plugins>
                     <plugin>
                        <artifactId>maven-deploy-plugin</artifactId>
                        <version>2.7</version>
                     </plugin>
                  </plugins>
               </pluginManagement>
            </build>
            <distributionManagement>
               <repository>
                  <id>cloudbees-adele-release</id>
                  <name>Cloudbees Private Repository</name>
                  <url>dav:https://repository-icasa.forge.cloudbees.com/release</url>
               </repository>
               <snapshotRepository>
                  <id>cloudbees-adele-release</id>
                  <name>Cloudbees Snapshot Repository</name>
                  <url>dav:https://repository-icasa.forge.cloudbees.com/snapshot</url>
               </snapshotRepository>
            </distributionManagement>
            <scm>
               <connection>scm:git:ssh://git@github.com/AdeleResearchGroup/ADELE-tools.git</connection>
               <url>http://github.com/AdeleResearchGroup/ADELE-tools/tree/master</url>
               <developerConnection>scm:git:ssh://git@github.com/AdeleResearchGroup/ADELE-tools.git</developerConnection>
               <tag>HEAD</tag>
            </scm>
         </project>
         